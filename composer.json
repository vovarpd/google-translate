{
    "name": "pixxet/google-translate",
    "description": "Free Laravel package for Paid Google Translate REST API",
    "keywords": ["laravel", "google", "translate", "php"],
    "license": "MIT",
    "homepage": "http://github.com/pixxet/google-translate",
    "authors": [
      {
        "name": "Omar Sharkeyeh",
        "email": "omar@pixxet.de"
      }
