    "require": {
        "php": ">=5.5.9|^8.0.2",
        "guzzlehttp/guzzle": "^6.1|^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2|^9.5"
    },
    "autoload": {
      "psr-4": {
